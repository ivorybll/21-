page{
  background:#F4F4F4; 
}
page view{
  overflow: hidden;
}
.Cheat{
  width: 590rpx;
  height:830rpx;
  margin: 0 auto;
  margin-top:40rpx;
  background: white;
  border-radius: 10rpx; 
  padding-top: 110rpx;
}
.Cheat text{
  font-size: 28rpx;
  width: 535rpx;
  height: 116rpx;
  text-align: center;
  display: block;
  margin: 0 auto; 
  line-height: 48rpx; 
}
.Cheat text:last-child{
  width: 100%;
  margin-top: 220rpx;
  text-align: center;
  font-size:24rpx; 
  font-family:PingFangSC-Regular;
  color:rgba(0,0,0,1);
}
.Cheat image{
  width: 447rpx;
  height: 297rpx;
  display: block;
  margin: 0 auto;
  margin-top: 125rpx;
}
.Cheatbottom{
  
  width:413rpx;
  margin: 0 auto;
  margin-top:60rpx;
}
.Cheatbottom button{
  width: 360rpx;
  height: 70rpx;
  border-radius: 10rpx;
  color:white;
  background:rgba(233,71,32,1);
  line-height: 70rpx;
  box-shadow:0px 0px 10px rgba(255,104,5,0.3);
  margin:0 auto;
  display: block;

}
.Cheatbottom text{
  font-size: 23rpx;
  font-family:PingFangSC-Regular;
  color:rgba(136,136,136,1);

}