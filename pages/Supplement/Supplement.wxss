page{
  width: 100%;
  height: 100%;
  background:#F4F4F4; 
}
.supplement{
  width: 590rpx;
  height: 830rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 40rpx;
  background: white;
  padding-top:110rpx; 
}
.supplement text{
  font-size: 28rpx;
  line-height: 48rpx;
  text-align: center;
  width: 541rpx;
  display: block;
  margin: 0 auto;
}
.supplement image{
  width: 427rpx;
  height: 352rpx;
  display: block;
  margin: 0 auto;
  margin-top: 142rpx;
}
.supplement text:last-child{
  margin-top:150rpx;
  font-size: 24rpx;
  text-align: center;
}
.supplementbottom{
  width: 413rpx;
  margin: 0 auto;  
  margin-top:60rpx;
}
.supplementbottom button{
  width: 360rpx;
  height: 70rpx;
  border:none;
  border-radius: 10rpx;
  background: rgba(233,71,32,1);
  color:white;
  line-height: 70rpx;
  box-shadow:0px 0px 10rpx rgba(255,104,5,0.3);
}
.supplementbottom text{
  font-size: 23rpx;
  color:#888888;
  width: 100%;
  text-align: center;
  margin-top: 20rpx;
}
